{
  "root": true,
  "extends": [
      "eslint:recommended",
      "plugin:json/recommended"
  ],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
      "project": "./tsconfig.json",
      "sourceType": "module"
  },
  "plugins": [
      "@typescript-eslint",
      "json"
  ],
  "rules": {
      "no-unused-vars": 2,
      "no-undef": 0,
      "func-names": 0,
      "no-eval": 2,
      "no-invalid-this": 0,
      "no-return-await": 1,
      "no-var": 0,
      "require-await": 2,
      "no-extra-semi": 1,
      "prefer-const": 2,
      "ban-ts-comment": 0,
      "no-prototype-builtins": 1
  }
}
